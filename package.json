{
  "name": "joyreact",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "fable-splitter -c splitter.config.js --define RELEASE",
    "start": "fable-splitter -c splitter.config.js -w --define DEBUG",
    "compile-for-test": "fable-splitter -c splitter.config.js --define TEST",
    "cold-start": "fable-splitter -c splitter.config.js --define DEBUG",
    "start-dev": "node node_modules/react-native/local-cli/cli.js start",
    "android": "node node_modules/react-native/local-cli/cli.js run-android"
  },
  "dependencies": {
    "buffer": "^5.2.1",
    "react": "^16.8.3",
    "react-native": "^0.59.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "fable-compiler": "^2.3.8",
    "fable-splitter": "^2.1.8",
    "metro-react-native-babel-preset": "^0.54.0"
  }
}
